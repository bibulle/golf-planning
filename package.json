  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "startessai": "GOLF_LOGIN_1=user_1 GOLF_PASSWORD_1=password_1 nx serve api",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "release-patch": "standard-version --no-verify --commit-all --release-as patch",
    "release-minor": "standard-version --no-verify --commit-all --release-as minor",
    "release-major": "standard-version --no-verify --commit-all --release-as major",
    "replace:version": "json -I -f libs/api-interfaces/src/lib/version.json -e \"this.version='`cat package.json | json version`';\" && git add libs/api-interfaces/src/lib/version.json"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run replace:version"
    }
  },
  "lint-staged": {
    "*.ts": [
